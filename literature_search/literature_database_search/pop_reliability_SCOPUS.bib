@ARTICLE{Kumar2022,
	author = {Kumar, Pankaj and Inamura, Yukako and Bao, Pham Ngoc and Abeynayaka, Amila and Dasgupta, Rajarshi and Abeynayaka, Helayaye D. L.},
	title = {Microplastics in Freshwater Environment in Asia: A Systematic Scientific Review},
