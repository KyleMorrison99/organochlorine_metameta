@ARTICLE{Stacy2024,
	author = {Stacy, Colzin and Amélie, Crépet and Blanche, Wies and Audrey, Rocabois and Margaux, Sanchez and Simon, Perreau and Julien, Jean and Matteo, Redaelli and Andreas, Kortenkamp and Christophe, Rousselle and Martine, Vrijheid and Mark, Nieuwenhuijsen and Rémy, Slama and Karine, Angeli},
