	publisher = {Medpharm Publications},
	issn = {1681150X},
	language = {English},
	abbrev_source_title = {S. Afr. Orthop. J.},
	type = {Review},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0; All Open Access, Gold Open Access}
}

