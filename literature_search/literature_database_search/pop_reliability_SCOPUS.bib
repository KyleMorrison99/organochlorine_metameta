	doi = {10.1016/j.envpol.2022.120828},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85143695392&doi=10.1016%2fj.envpol.2022.120828&partnerID=40&md5=7255af25096be01a695baa345e4fd1f1},
