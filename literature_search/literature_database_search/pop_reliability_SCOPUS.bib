@ARTICLE{Marí-Bauset20226,
	author = {Marí-Bauset, Salvador and Peraita-Costa, Isabel and Donat-Vargas, Carolina and Llopis-González, Agustín and Marí-Sanchis, Amelia and Llopis-Morales, Juan and Morales Suárez-Varela, María},
