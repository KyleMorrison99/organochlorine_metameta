@ARTICLE{Xu20223565,
	author = {Xu, Libo and Hu, Min and Jia, Weiqian and Zhang, Mengjun and Tang, Qian and Tian, Xudong and Huang, Yi},
	title = {Distribution and transport of atmospheric microplastics and the environmental impacts: A review; [大气环境中微塑料分布与迁移及生态环境影响研究进展]},
	year = {2022},
	journal = {Kexue Tongbao/Chinese Science Bulletin},
