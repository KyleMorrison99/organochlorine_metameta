	title = {Linkages of volatile fatty acids and polyhexamethylene guanidine stress during sludge fermentation: Metagenomic insights of microbial metabolic traits and adaptation},
	year = {2023},
	journal = {Chinese Chemical Letters},
	volume = {34},
