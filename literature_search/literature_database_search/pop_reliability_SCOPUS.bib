@ARTICLE{Li2023872,
	author = {Li, Diying and Sun, Chunxiao and Liu, Xia and Dai, Yanhui and Zhao, Jian},
	title = {Interaction between per- and polyfluoroalkyl substances and microorganisms; [全氟和多氟烷基物质与微生物相互作用的研究进展]},
	year = {2023},
